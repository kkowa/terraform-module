{
  "name": "kkowa/terraform-module",
  "dockerComposeFile": [
    "../docker-compose.yml",
    "./override.yml"
  ],
  "runServices": [
    "workspace"
  ],
  "service": "workspace",
  "overrideCommand": true,
  "initializeCommand": "sh ./.devcontainer/initializeCommand.sh",
  "onCreateCommand": "sh ./.devcontainer/onCreateCommand.sh",
  "updateContentCommand": "sh ./.devcontainer/updateContentCommand.sh",
  "postCreateCommand": "sh ./.devcontainer/postCreateCommand.sh",
  "postStartCommand": "sh ./.devcontainer/postStartCommand.sh",
  "postAttachCommand": "sh ./.devcontainer/postAttachCommand.sh",
  "shutdownAction": "stopCompose",
  "workspaceFolder": "/var/workspace",
  "extensions": [
    "mhutchie.git-graph",
    "eamodio.gitlens",
    "ldez.ignore-files",
    "gruntfuggly.todo-tree",
    "redhat.vscode-yaml",
    "tamasfe.even-better-toml",
    "streetsidesoftware.code-spell-checker",
    "editorconfig.editorconfig",
    "golang.go",
    "hashicorp.terraform",
    "ms-azuretools.vscode-docker",
    "ms-kubernetes-tools.vscode-kubernetes-tools"
  ]
}
